{
	"title": "COVID19",
	"name": "covid-19-personal",
	"version": "1.1.2",
	"scripts": {
		"build": "npm run clean && npm run build:pug && npm run build:scss && npm run minify-css && npm run build:scripts && npm run build:assets",
		"build:assets": "node scripts/build-assets.js",
		"build:pug": "node scripts/build-pug.js",
		"build:scripts": "babel src/js -d dist/js",
		"build:scss": "node scripts/build-scss.js",
		"clean": "node scripts/clean.js",
		"start": "npm run build && node scripts/start.js",
		"start:debug": "npm run build && node scripts/start-debug.js",
		"minify-css": "postcss dist/css/styles.css > dist/css/styles.min.css"
	},

	"author": "Swapnil Shah",
	"dependencies": {
		"bootstrap": "^4.4.1",
		"dotenv": "^8.2.0"
	},
	"devDependencies": {
		"autoprefixer": "9.7.4",
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"browser-sync": "2.26.7",
		"chokidar": "3.3.1",
		"concurrently": "5.1.0",
		"cssnano": "^4.1.10",
		"postcss": "7.0.27",
		"postcss-cli": "^7.1.0",
		"prettier": "1.19.1",
		"pug": "2.0.4",
		"sass": "1.26.0",
		"shelljs": "0.8.3",
		"upath": "1.2.0"
	}
}
