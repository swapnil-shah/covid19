{
	"title": "COVID19",
	"name": "covid-19-personal",
	"version": "1.1.2",
	"scripts": {
		"build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets && npm run postcss && npm run minify-scripts",
		"build:assets": "node scripts/build-assets.js",
		"build:pug": "node scripts/build-pug.js",
		"build:scripts": "babel src/js -d dist/js",
		"build:scss": "node scripts/build-scss.js && npm run postcss",
		"clean": "node scripts/clean.js",
		"start": "npm run build && node scripts/start.js ",
		"start:debug": "npm run build && node scripts/start-debug.js",
		"postcss": "postcss dist/css/styles.css -o dist/css/styles.min.css",
		"terser-index": "terser dist/js/utilities.js dist/js/scripts.js dist/js/dashboard.js -o dist/js/dashboard.min.js -c",
		"terser-us": "terser dist/js/utilities.js dist/js/scripts.js dist/js/in.js -o dist/js/in.min.js -c",
		"terser-india": "terser dist/js/utilities.js dist/js/scripts.js dist/js/us.js -o dist/js/us.min.js -c",
		"minify-scripts": "npm run terser-index && npm run terser-us && npm run terser-india"
	},
	"author": "Swapnil Shah",
	"dependencies": {
		"autoprefixer": "9.7.4",
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"bootstrap": "^4.4.1",
		"chokidar": "3.3.1",
		"concurrently": "5.1.0",
		"cssnano": "^4.1.10",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"postcss": "7.0.27",
		"postcss-cli": "^7.1.0",
		"prettier": "1.19.1",
		"pug": "2.0.4",
		"sass": "1.26.0",
		"shelljs": "0.8.3",
		"upath": "1.2.0"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^2.2.0",
		"browser-sync": "2.26.7",
		"purgecss": "^2.2.1",
		"tailwindcss": "^1.4.5"
	}
}
