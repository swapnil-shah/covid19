    fetch('https://api.rootnet.in/covid19-in/stats/latest', { cache: 'no-cache' })
        .then((response) => response.json())
        .then((data) => {
            countiesData(data);
        });
    function countiesData(data) {
        var mymap = L.map('covidCountryMap').setView([ 20.59, 78.96 ], 4);
        let mapboxAccessToken =
            'pk.eyJ1Ijoic3dhcG5pbDciLCJhIjoiY2s5ZnRtZXh1MGVuYTNkbGNhNWtrcmt0diJ9.fzy5HcciZYupAiuzpVmlWA';
        L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token=' + mapboxAccessToken, {
            id: 'mapbox/dark-v10',
            attribution:
                'Map data <a href="https://disease.sh/">Novel COVID API</a>, Imagery Â© <a href="https://www.mapbox.com/">Mapbox</a>',
            minZoom: 1,
            noWrap: true,
            bounds: [ [ -90, -180 ], [ 90, 180 ] ]
        }).addTo(mymap);
        var myRenderer = L.canvas({ padding: 0.5 });
        function htmlPopup(county, province, confirmed, recovered, deaths) {
            let output = '';
            if (county !== null) {
                output += `<b>${county}</b>, `;
            }
            if (province !== null) {
                output += `<b>${province}</b>`;
            }
            if (confirmed !== null) {
                output += `<br/>Confirmed: ${confirmed.toLocaleString()}`;
            }
            if (recovered !== null) {
                output += `<br/>Recovered: ${recovered.toLocaleString()}`;
            }
            if (deaths !== null) {
                output += `<br/>Deaths: ${deaths.toLocaleString()}`;
            }
            return output;
        }
        data.data.regional.forEach(function(state) {
        console.log("county", state)
            var latlng = L.latLng({ lat: statesIndiaLatLong(state), lng: statesIndiaLatLong(state) });
            var circle = L.circleMarker(latlng, {
                fillOpacity: 0.3,
                radius: 5,
                stroke: false,
                fillColor: '#ff8725',
                renderer: myRenderer
            })
                .addTo(mymap)
                .bindTooltip(
                    htmlPopup(
                        county.county,
                        county.province,
                        county.stats.confirmed,
                        county.stats.recovered,
                        county.stats.deaths
                    )
                );
        });
        var info = L.control({ position: 'bottomright' });

        info.onAdd = function(map) {
            this._div = L.DomUtil.create('div', 'map-info'); // create a div with a class "info"
            this._div.innerHTML = '<h4>COVID19 USA SPREAD DATA</h4><p class="mb-0">Tap/hover on the marker for more information</p>';
            return this._div;
        };
        info.addTo(mymap);
    }
