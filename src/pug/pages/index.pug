            .col-sm-12
                .card.mb-4
                    .card-header.bg-dark.d-block
                        h5.text-white.mb-0  Travel Health Notices
                    .card-body
                        ul.nav.nav-pills.card-header-pills#cardPill(role='tablist').pt-3.pb-5.m-0.d-flex.justify-content-between.justify-content-md-start
